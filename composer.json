    "name": "open-data/od",
    "description": "Project template for Drupal 8 sites built with the Open Data distribution.",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
